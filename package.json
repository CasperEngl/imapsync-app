{
  "name": "imapsync",
  "version": "0.6.1",
  "homepage": "./",
  "private": true,
  "author": "Casper Engelmann",
  "description": "Application to make commands for imapsync by Gilles LAMIRAL",
  "main": "public/electron.js",
  "build": {
    "appId": "com.casperengelmann.imapsync",
    "productName": "Imapsync",
    "copyright": "Copyright Â© 2019 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraFiles": [
      {
        "from": "resources/${os}",
        "to": "Resources/bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.png"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Imapsync",
      "artifactName": "imapsync.${version}.${ext}"
    }
  },
  "scripts": {
    "cra:start": "react-scripts start",
    "cra:build": "react-scripts build",
    "cra:test": "react-scripts test --env=jsdom",
    "cra:eject": "react-scripts eject",
    "electron:dev": "concurrently \"yarn ts:build\" \"BROWSER=none yarn cra:start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:pack": "run-s 'electron:prebuild' 'electron:build -mwl'",
    "electron:prebuild": "yarn ts:build && yarn cra:build",
    "electron:build": "electron-builder -c.extraMetadata.main=build/electron.js",
    "electron:start": "electron .",
    "ts:build": "tsc --noEmit false --module commonjs",
    "ts:watch": "tsc -w --noEmit false --module commonjs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.13",
    "@fortawesome/free-solid-svg-icons": "^5.7.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "base-64": "^0.1.0",
    "bootstrap": "^4.1.0",
    "electron-is-dev": "^1.0.1",
    "electron-preferences": "^1.2.0",
    "electron-serve": "^0.2.0",
    "find-process": "^1.2.1",
    "font-awesome": "^4.7.0",
    "is-valid-json": "^1.0.2",
    "jquery": "^3.3.1",
    "mkdirp": "^0.5.1",
    "node-notifier": "^5.3.0",
    "popper.js": "^1.14.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-perfect-scrollbar": "^1.4.4",
    "react-redux": "^6.0.0",
    "react-scripts": "^2.1.3",
    "react-spring": "^7.2.8",
    "reactstrap": "^7.0.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-localstorage-simple": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^4.1.3",
    "tinycolor2": "^1.4.1",
    "typesafe-actions": "^3.0.0",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "@types/base-64": "^0.1.2",
    "@types/electron-debug": "^1.1.0",
    "@types/electron-is-dev": "^0.3.0",
    "@types/jest": "^23.3.13",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.12.18",
    "@types/node-notifier": "^0.0.28",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-redux": "^7.0.0",
    "@types/reactstrap": "^7.1.0",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^4.1.6",
    "@types/tinycolor2": "^1.4.1",
    "@types/validator": "^10.9.0",
    "@typescript-eslint/parser": "^1.1.0",
    "concurrently": "^4.1.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "typescript-eslint-parser": "^22.0.0",
    "wait-on": "^3.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
