{
  "name": "imapsync",
  "version": "1.0.0",
  "homepage": "./",
  "private": true,
  "author": "Casper Engelmann",
  "description": "Application to make commands for imapsync by Gilles LAMIRAL",
  "main": "public/electron.js",
  "build": {
    "appId": "dk.web-koncept.imapsync",
    "productName": "Imapsync",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraFiles": [
      {
        "from": "resources/${os}",
        "to": "Resources/bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.png"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Imapsync",
      "artifactName": "dk.web-koncept.imapsync.${version}.${ext}"
    }
  },
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "babel-eslint": "9.0.0",
    "bootstrap": "^4.1.0",
    "bootswatch": "^4.1.0",
    "debug": "^4.1.1",
    "electron": "^4.0.0",
    "electron-is-dev": "^1.0.1",
    "eslint": "5.6.0",
    "font-awesome": "^4.7.0",
    "gsap": "^2.0.1",
    "jquery": "^3.3.1",
    "node-notifier": "^5.3.0",
    "node-sass-chokidar": "^1.2.2",
    "popmotion": "^8.3.8",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-https-redirect": "^1.0.10",
    "react-pose": "^4.0.4",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.2",
    "react-transition-group": "^2.4.0",
    "reactstrap": "^7.0.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-localstorage-simple": "^2.0.0",
    "redux-throttle": "^0.1.1",
    "redux-thunk": "^2.2.0",
    "shelljs": "^0.8.3",
    "styled-components": "^4.1.3",
    "validator": "^10.10.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/style",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/style --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:pack": "run-s electron:prepack electron:build",
    "electron:prepack": "yarn build",
    "electron:build": "electron-builder -c.extraMetadata.main=build/electron.js -mwl",
    "electron:start": "electron ."
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron-builder": "^20.38.4",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "foreman": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "wait-on": "^3.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
